#!/usr/bin/env bash
REST=$@

rosrun packlo packlo \
    --alsologtostderr=1 \
    --v=3 \
		--enable_debug=true \
		--registration_algorithm=sph-mock-translated \
		--spherical_bandwith=128 \
    --sampling_neighbors=4 \
		--mock_translate_x=42.0 \
		--mock_translate_y=48.0 \
		--mock_translate_z=35.0 \
    --phase_discretize_lower=-150 \
    --phase_discretize_upper=150 \
    --phase_n_voxels=301 \
    --z_score_lag=400 \
    --z_score_threshold=10.5 \
    --z_score_influence=0.1 \
    $REST
